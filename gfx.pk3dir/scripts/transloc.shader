
// Red
hg_transloc_team1
{
    deformVertexes wave 1 sin 0.1 0.5 0 1.25
    {
        map textures/hg_transloc_team1.tga
        tcMod rotate 30
        tcMod scale 0.3 0.3
        tcMod scroll 0.1 0.001
        tcGen environment
        blendfunc add
        rgbgen identity
    }
}

